{
	"name": "sanchalit-apps",
	"version": "0.2.0",
	"private": false,
	"description": "Sanchalit application suite",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"analyze": "webpack --profile --json=stats.json --analyze",
		"build": "npm run build:web",
		"build:chromium": "NODE_ENV=production BUILD_TARGET=chromium webpack",
		"build:firefox": "NODE_ENV=production BUILD_TARGET=firefox webpack",
		"build:safari": "NODE_ENV=production BUILD_TARGET=safari webpack",
		"build:web": "NODE_ENV=production BUILD_TARGET=web webpack",
		"dev": "npm run dev:web",
		"dev:chromium": "NODE_ENV=development BUILD_TARGET=chromium webpack --watch",
		"dev:firefox": "NODE_ENV=development BUILD_TARGET=firefox webpack --watch",
		"dev:safari": "NODE_ENV=development BUILD_TARGET=safari webpack --watch",
		"dev:web": "NODE_ENV=development BUILD_TARGET=web webpack serve"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/abhijeetpandit7/sanchalit-apps.git"
	},
	"author": "Abhijeet Pandit",
